___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const inject = require('injectScript');
const setInWindow = require('setInWindow');
const copyFromWindow = require('copyFromWindow');

const config = copyFromWindow('fast_config') || {};

config.includedFromGtm = true;

for(const key in data.config) {
  const item = data.config[key];
  config[item.name] = item.value;
}

if (data.domainAlias !== '') {
  config.domainAlias = data.domainAlias;
}
if (data.serverId) {
  config.experiments = { serverSide: true };
}

setInWindow("fast_config", config, true);

inject('https://fast.smarketer.de/api/js/fast.js', () => {
  data.gtmOnSuccess();
});


